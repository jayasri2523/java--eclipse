<!-- <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd"> -->
			
		
			 
			 <beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
			 
			 
			<bean name="garden" class="com.jobiak.ioc.Garden"> 
			<property name="fruits">
			      <list>
			           <value>Mango</value>
                        <value>Kiwi</value>		
                        <value> musk melon</value>	    
                        <value>water melon</value>     
                        <value>orange</value>  
			           
			           </list>
			
			</property>
			
			<property name="trees">
			
			<set>
			            <value>Banyan Tree</value>
                        <value>The Oak Tree</value>		
                        <value> The Lebanon Tree</value>	    
                        <value>The Neem Tree</value>     
                        <value>The Tamarind</value>  
			 
			</set>
			</property>
			
			
			<property name="cityFruit">
			 
			    <map>
			        <entry key="hyderabad" value="custardapple"></entry>
			        <entry key="banginapally" value="mangoes"></entry>
			        <entry key="narayanapur" value="grapes"></entry>
			        <entry key="ananthapur" value="muskmelon"></entry>
			        <entry key="rajuhmandry" value="banana"></entry>
			    
			    
			    </map>
			
			
			
			
			
			</property>
			
			
		
			
			<constructor-arg name="flowers">
			
			<list>
			
			
                        <value>Jasmine</value>		
                        <value>Rose</value>	    
                        <value>Lilly</value>     
                        <value>Cosmos</value>
                        <value>Chrisanthum</value> 
			
			
			
			
			
			</list>
			
			
			
			</constructor-arg>
			
			
			
			
			
			
			
			
			
			
			</bean>
			
			</beans>