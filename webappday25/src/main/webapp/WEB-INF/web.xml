  <servlet>
  
  <servlet-name>jdbcServlet</servlet-name>
  <servlet-class>ai.jobiak.webapp.jdbcServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>jdbcServlet</servlet-name>
  <url-pattern>/connect</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Factorial</servlet-name>
  <servlet-class>ai.jobiak.webapp.Factorial</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Factorial</servlet-name>
  <url-pattern>/Fact</url-pattern>
  </servlet-mapping>
  
  <servlet>
  
  <servlet-name>jdbcServlet2</servlet-name>
  <servlet-class>ai.jobiak.webapp.jdbcServlet2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>jdbcServlet2</servlet-name>
  <url-pattern>/connection</url-pattern>
  </servlet-mapping>
  
   <servlet>
  
  <servlet-name>Lifecycle</servlet-name>
  <servlet-class>ai.jobiak.webapp.Lifecycle</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Lifecycle</servlet-name>
  <url-pattern>/go</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
          <welcome-file>links.html</welcome-file>
          
  </welcome-file-list>

  
  
</web-app>
