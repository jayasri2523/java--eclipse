package ai.jobiak.IceCream;

import java.util.stream.IntStream;

public class ChocolateIcecream {
	Icecream Chocolate;
	String brand;
	

	public ChocolateIcecream() {
		// TODO Auto-generated method stub

	}


	public ChocolateIcecream(Icecream chocolate, String brand) {
		super();
		Chocolate = chocolate;
		this.brand = brand;
	}


	public Icecream getChocolate() {
		return ChocolateIcecream;
	}


	public void setChocolate(IntStream chocolate) {
		Chocolate = chocolate;
	}


	public String getBrand() {
		return brand;
	}


	public void setBrand(String brand) {
		this.brand = brand;
	}


	@Override
	public String toString() {
		return "ChocolateIcecream [brand=" + brand + "]";
	}
	
	
	
	

}
